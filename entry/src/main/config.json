{
  "app": {
    "bundleName": "com.example.myapplication",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {
    "default": {
      "directLaunch": false,
      "network": {
        "cleartextTraffic": true
      }
    }
  },
  "module": {
    "package": "com.example.myapplication",
    "name": ".MyApplication",
    "mainAbility": "com.example.myapplication.MainAbility",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "name": "com.example.myapplication.MainAbility",
        "description": "$string:mainability_description",
        "icon": "$media:icon",
        "label": "$string:entry_MainAbility",
        "launchType": "standard",
        "orientation": "unspecified",
        "visible": true,
        "type": "page"
      },
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "name": "com.example.myapplication.loginAbility",
        "description": "$string:loginability_description",
        "icon": "$media:icon",
        "label": "$string:entry_loginAbility",
        "launchType": "standard",
        "orientation": "unspecified",
        "visible": true,
        "type": "page"
      },
      {
        "name": "com.example.myapplication.textAbility",
        "description": "$string:textability_description",
        "icon": "$media:icon",
        "label": "$string:entry_textAbility",
        "launchType": "standard",
        "orientation": "unspecified",
        "type": "page"
      },
      {
        "name": "com.example.myapplication.RegisterAbility",
        "description": "$string:registerability_description",
        "icon": "$media:icon",
        "label": "$string:entry_RegisterAbility",
        "launchType": "standard",
        "orientation": "unspecified",
        "type": "page"
      },
      {
        "name": "com.example.myapplication.ProductDetailAbility",
        "description": "$string:productdetailability_description",
        "icon": "$media:icon",
        "label": "$string:entry_ProductDetailAbility",
        "launchType": "standard",
        "orientation": "unspecified",
        "type": "page"
      }
    ],
    "reqPermissions": [
      {
        "name": "ohos.permission.INTERNET"
      }
    ]
  }
}